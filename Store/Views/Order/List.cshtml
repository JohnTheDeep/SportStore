@model IEnumerable<Order>

@{ 
    ViewBag.Title = "Orders";
    Layout = "_AdminLayout";
}
<body>
    @if (Model.Count() > 0)
    {
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Country</th>
                    <th>State</th>
                    <th>City</th>
                    <th>Zip</th>
                    <th>Adress(Line 1)</th>
                    <th>Adress(Line 1)</th>
                    <th>Adress(Line 1)</th>
                    <th>Gift?</th>
                </tr>
            </thead>
        </table>
    }
</body>
